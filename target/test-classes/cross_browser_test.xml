<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="TestSuite" thread-count="3" parallel="tests">
	<test name="ChromeTest">
		<parameter name="browser" value="Chrome" />
		<parameter name="url" value="http://secure.smartbearsoftware.com/samples/TestComplete11/WebOrders/Login.aspx"/>
		<classes>
			<class name="com.WebOrder.cross_browser_test_using_baseClass_multiData" />
		</classes>
	</test>
	<test name="FirefoxTest">
		<parameter name="browser" value="Firefox" />
		<parameter name="url" value="http://secure.smartbearsoftware.com/samples/TestComplete11/WebOrders/Login.aspx"/>
		
		<classes>
			<class name="com.WebOrder.cross_browser_test_using_baseClass_multiData" />
		</classes>
	</test>
	<test name="EdgeTest">
		<parameter name="browser" value="Edge" />
		<parameter name="url" value="http://secure.smartbearsoftware.com/samples/TestComplete11/WebOrders/Login.aspx"/>
		
		<classes>
			<class name="com.WebOrder.cross_browser_test_using_baseClass_multiData" />
		</classes>
	</test>
</suite>